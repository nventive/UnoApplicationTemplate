<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Style x:Key="DefaultHyperlinkButtonStyle"
		   BasedOn="{StaticResource MaterialHyperlinkButtonStyle}"
		   TargetType="HyperlinkButton">
		<Setter Property="Foreground"
				Value="{StaticResource MaterialOnBackgroundMediumBrush}" />
		<Setter Property="Background"
				Value="Transparent" />
		<Setter Property="MinHeight"
				Value="17" />
		<Setter Property="FontFamily"
				Value="{StaticResource FontFamily02}" />
		<Setter Property="FontSize"
				Value="12" />
		<Setter Property="UseSystemFocusVisuals"
				Value="True" />
	</Style>

	<Style TargetType="HyperlinkButton"
		   BasedOn="{StaticResource DefaultHyperlinkButtonStyle}" />

</ResourceDictionary>
