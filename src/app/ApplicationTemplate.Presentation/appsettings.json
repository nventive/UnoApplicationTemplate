{
	"Serilog": {
		"MinimumLevel": {
			"Default": "Information",
			"Override": {
				"ApplicationTemplate": "Information"
			}
		}
	},
	"LoggingOutput": {
		"IsConsoleLoggingEnabled": true,
		"IsFileLoggingEnabled": true
	},
	"Mock": {
		"IsMockEnabled": false,
		"IsDelayForSimulatedApiCallsEnabled": true
	},
	"ApplicationStoreUrls": {
		"IOS": "https://apps.apple.com/us/app/uno-calculator/id1464736591",
		"Android": "https://play.google.com/store/apps/details?id=uno.platform.calculator"
	},
	"Agentic": {
		"Endpoint": "https://your-ai-foundry-endpoint.azure.com/v1/chat/completions",
		"ApiKey": "your-api-key-here",
		"DeploymentName": "gpt-4",
		"Temperature": 0.7,
		"MaxTokens": 1000,
		"SpeechEndpoint": "https://your-speech-service.cognitiveservices.azure.com",
		"SpeechApiKey": "your-speech-api-key-here",
		"VoiceInputEnabled": true,
		"VoiceOutputEnabled": true
	}
}